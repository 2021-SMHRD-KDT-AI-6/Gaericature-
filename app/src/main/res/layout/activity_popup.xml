<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/radius"
    tools:context=".Activity.PopupActivity">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/twayair"
        android:text="주소 추가"
        android:textColor="@color/orange"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/twayair"
        android:text="이름"
        android:textColor="@color/orange"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <EditText
        android:id="@+id/edtDname"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="15dp"
        android:background="@drawable/edt1"
        android:ems="10"
        android:hint="수령인"
        android:inputType="textPersonName"
        android:paddingLeft="5dp"
        android:textSize="14dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/tvNull"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/twayair"
        android:text="배송지명"
        android:textColor="@color/orange"
        app:layout_constraintStart_toStartOf="@+id/edtDname"
        app:layout_constraintTop_toBottomOf="@+id/edtDname" />

    <EditText
        android:id="@+id/edtTag"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:background="@drawable/edt1"
        android:ems="10"
        android:hint="배송지명"
        android:inputType="textPersonName"
        android:paddingLeft="5dp"
        android:textSize="14dp"
        app:layout_constraintEnd_toEndOf="@+id/edtDname"
        app:layout_constraintStart_toStartOf="@+id/tvNull"
        app:layout_constraintTop_toBottomOf="@+id/tvNull"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/edtPhone"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:background="@drawable/edt1"
        android:ems="10"
        android:hint=" - 없이 숫자만 입력"
        android:inputType="phone"
        android:paddingLeft="5dp"
        android:textSize="14dp"
        app:layout_constraintEnd_toEndOf="@+id/edtTag"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/twayair"
        android:text="연락처"
        android:textColor="@color/orange"
        app:layout_constraintStart_toStartOf="@+id/edtTag"
        app:layout_constraintTop_toBottomOf="@+id/edtTag" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/twayair"
        android:text="주소"
        android:textColor="@color/orange"
        app:layout_constraintStart_toStartOf="@+id/edtPhone"
        app:layout_constraintTop_toBottomOf="@+id/edtPhone" />

    <EditText
        android:id="@+id/edtAddr"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:background="@drawable/edt1"
        android:ems="10"
        android:hint="주소"
        android:inputType="textPersonName"
        android:paddingLeft="5dp"
        android:textSize="14dp"
        app:layout_constraintEnd_toEndOf="@+id/edtPhone"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <Button
        android:id="@+id/btnAdd"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/btn1"
        android:fontFamily="@font/twayair"
        android:gravity="center"
        android:text="완 료"
        android:textColor="@color/orange"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtAddr" />

</androidx.constraintlayout.widget.ConstraintLayout>